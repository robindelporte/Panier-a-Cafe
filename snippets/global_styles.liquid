/* Animation uniquement sur desktop (&ge; 992px) */
@media (min-width: 992px) {
  /* Position initiale de product_atc-form */
  .product_atc-form {
    transform: translateY(3.5rem);
    transition: transform 0.4s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  }
  /* Au hover du parent product-item_component */
  .product-item_component:hover .product_atc-form {
    transform: translateY(0rem);
  }
  /* &Eacute;tat initial de product_quick-atc */
  .product_quick-atc {
    opacity: 1;
    display: flex;
    transition: opacity 0.35s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  }
  /* Au hover de product_atc-form - opacity puis display none apr&egrave;s SEULEMENT si pas .is-no-variant */
  .product_atc-form:hover .product_quick-atc:not(.is-no-variant) {
    opacity: 0;
    transition: opacity 0.35s cubic-bezier(0.25, 0.46, 0.45, 0.94), display 0s 0.35s;
    display: none;
  }
  /* Masquer le nom de variante au hover */
  .product-atc_radio .variant-name {
    display: inline;
  }
  .product-atc_radio:hover .variant-name {
    display: none;
  }
  /* Afficher le prix au hover */
  .variant-price {
    display: none;
  }
  .product-atc_radio:hover .variant-price {
    display: inline;
  }
}
/* Sur mobile/tablette (< 992px) - &eacute;tat statique */
@media (max-width: 991px) {
  .product_atc-form {
    transform: translateY(0rem);
  }
  .product_quick-atc {
    opacity: 1;
    display: flex;
  }
  .product-atc_radio .variant-name {
    display: inline;
  }
  .variant-price {
    display: none;
  }
}
