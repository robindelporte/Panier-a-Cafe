          576: {
            perPage: 1.2,
            gap: '12px',
          },
          480: {
            perPage: 1,
            gap: '8px',
          }
        }
      });
      splide.mount();
      splideInstances[index] = splide;
    }
  });
}
document.addEventListener('DOMContentLoaded', initSplideSliders);
