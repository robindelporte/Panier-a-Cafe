                        alt="{{ section.settings.image_badges_certification_cafe_image.alt }}"
                        class="icon-embed-xsmall is-cover"
                      >
                      <div class="product_header-tag-text">
                        {{ section.settings.text_badges_certification_cafe_texte }}
                      </div>
                    </div>
                  {% endif -%}
