<div class="padding-global padding-section-small is-hero-product">
  <div class="container-large">
    <div class="product-header_component">
      <script src="{{ 'liquify_custom.js' | asset_url }}" type="text/javascript"></script>
      <div
        data-section-id="{{ section.id }}"
        li-element="product-variant-container"
        class="w-layout-grid product-header_layout"
        x-data="handleVariant({{ product | json | escape }}, {{ product.options_by_name | json | escape }}, {{ product.selected_or_first_available_variant | json | escape }})"
        x-init="$nextTick(() => { activateVariants() })"
        @setbundleitem.window="updateBundleItem($event.detail.product, $event.detail.quantity)"
      >
        <div id="w-node-_9aaa613b-8e17-c44d-2144-26a60c99cba5-0a9591df" class="swiper-container is-product-gallery">
          <div class="swiper is-product-gallery">
            <div class="swiper_button-wrapper is-product-slider">
              <a
                aria-label="Previous Slide"
                href="#"
                class="swiper_button swiper-prev pointer-events-on w-inline-block"
              >
                <div class="swiper_button-icon w-embed">
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    width="100%"
                    height="100%"
                    viewbox="0 0 29.541 22.323"
                    style="transform: rotate(180deg)"
                  >
                    <g id="Group_44" data-name="Group 44" transform="translate(0 1.014)"><path id="Path_1" data-name="Path 1" d="M115.445,20.633l9.311-10.148L115.445.338" transform="translate(-97.25 -0.338)" fill="none" stroke="currentColor" stroke-miterlimit="10" stroke-width="3"></path><line id="Line_1" data-name="Line 1" x1="27.506" transform="translate(0 10.148)" fill="none" stroke="currentColor" stroke-miterlimit="10" stroke-width="3"></line></g>
                  </svg>
                </div></a
              ><a aria-label="Next Slide" href="#" class="swiper_button swiper-next pointer-events-on w-inline-block">
                <div class="swiper_button-icon w-embed">
                  <svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100%" viewbox="0 0 29.541 22.323">
                    <g id="Group_44" data-name="Group 44" transform="translate(0 1.014)"><path id="Path_1" data-name="Path 1" d="M115.445,20.633l9.311-10.148L115.445.338" transform="translate(-97.25 -0.338)" fill="none" stroke="currentColor" stroke-miterlimit="10" stroke-width="3"></path><line id="Line_1" data-name="Line 1" x1="27.506" transform="translate(0 10.148)" fill="none" stroke="currentColor" stroke-miterlimit="10" stroke-width="3"></line></g>
                  </svg>
                </div>
              </a>
            </div>
            <div id="my-gallery" class="swiper-wrapper is-product-gallery">
              {% for image in product.images %}
                <div li-for="image in product.images" class="swiper-slide is-product-gallery">
                  <div class="product-header_gallery-lightbox">
                    <img
                      li-object:src="image.src | product_img_url: 'master'"
                      li-object:alt="image.alt"
                      alt="{{ image.alt }}"
                      src="{{ image.src | product_img_url: 'master' }}"
                      loading="lazy"
                      class="product-header_gallery-image"
                    >
                  </div>
                </div>
              {% endfor %}
            </div>
            <div class="swiper_pagination is-product-slider is-mixed">
              <div class="swiper_bullet is-active"></div>
              <div class="swiper_bullet"></div>
            </div>
          </div>
          <div class="product-header_slider-wrapper">
            <div class="swiper is-product-thumbnail">
              <div class="swiper-wrapper is-product-thumbnail">
                {% for image in product.images %}
                  <div li-for="image in product.images" class="swiper-slide is-product-thumbnail">
                    <img
                      li-object:src="image.src | product_img_url: 'master'"
                      li-object:alt="image.alt"
                      alt="{{ image.alt }}"
                      src="{{ image.src | product_img_url: 'master' }}"
                      loading="lazy"
                      class="product_thumbnail-image"
                    >
                  </div>
                {% endfor %}
              </div>
            </div>
            <div class="swiper-slide is-product-thumbnail is-active hide"></div>
          </div>
          {% if product.type == 'Cafe' %}
            <div li-if="product.type == 'Cafe'" class="product_flag-wrapper">
              <img
                src="{{ section.settings.image_settingsimage | img_url: 'master' }}"
                loading="lazy"
                alt="{{ section.settings.image_settingsimage.alt }}"
                class="product_flag"
              >
            </div>
          {% endif %}
        </div>
        <div
          bss-b2b-main-product-form=""
          x-data="{selling_plan: '', sellingplan_active: false}"
          class="product-header_product-details"
        >
          <ul role="list" class="breadcrumbs_wrapper">
            <li class="breadcrumbs_item">
              <a li-object:href="routes.root_url" href="{{ routes.root_url }}" class="breadcrumbs_link">Accueil</a>
              <div class="breadcrumbs_separator">-</div>
            </li>
            {% if product.collections.size > 0 %}
              <li li-if="product.collections.size > 0" class="breadcrumbs_item">
                <a
                  li-object:href="product.collections[0].url"
                  href="{{ product.collections[0].url }}"
                  class="breadcrumbs_link w-inline-block"
                >
                  <div li-object="product.collections[0].title">{{ product.collections[0].title }}</div>
                </a>
                <div class="breadcrumbs_separator">-</div>
              </li>
            {% endif %}
            <li class="breadcrumbs_item">
              <div li-object="product.title">{{ product.title }}</div>
            </li>
          </ul>
          <h1 li-object:text="product.title" class="heading-style-h2">
            {{ product.title }}
          </h1>
          <div class="product-header_price-wrapper">
            <div class="product-header_price-container" style="display:flex;justify-content:space-between;align-items:center;width:100%;">
  
  <!-- prix + prix/kg restent groupés -->
  <div class="product-header_price">
    <div bss-b2b-item-original-price="" bss-b2b-cart-item-key="{{ item.key }}" class="product_price">
      <div
        bss-b2b-featured-product=""
        li-js-price="product.selected_or_first_available_variant.price"
        bss-b2b-product-id="{{ product.id }}"
        bss-b2b-product-price=""
        bss-b2b-variant-price=""
        bss-b2b-variant-id="{{ product.selected_or_first_available_variant.id }}"
        class="text-size-medium"
        x-text="LiquifyHelper.moneyFormat(product.selected_or_first_available_variant.price, true, '{{ shop.money_with_currency_format }}')"
      >
        12,40€
      </div>
      <template x-if="selected_or_first_available_variant.compare_at_price > selected_or_first_available_variant.price">
        <div li-js-if="selected_or_first_available_variant.compare_at_price > selected_or_first_available_variant.price">
          <div
            li-object="product.compare_at_price | money"
            class="text-size-regular text-style-strikethrough text-align-right text-style-muted"
          >
            {{ product.compare_at_price | money }}
          </div>
        </div>
      </template>
    </div>

    {% if product.selected_or_first_available_variant.unit_price %}
      <div li-if="product.selected_or_first_available_variant.unit_price" class="product_price-per-kilo">
        <div>–</div>
        <div data-price-per-kg="">
          <span li-object="product.selected_or_first_available_variant.unit_price | money">
            {{- product.selected_or_first_available_variant.unit_price | money -}}
          </span>
          {%- if product.selected_or_first_available_variant.unit_price_measurement.reference_unit %}
            <span li-if="product.selected_or_first_available_variant.unit_price_measurement.reference_unit">/kg</span>
          {% endif %}
        </div>
      </div>
    {% endif %}
  </div>

  {%- for block in section.blocks -%}
    {%- case block.type -%}
      {%- when '@app' -%}
        <div class="app-slot" {{ block.shopify_attributes }}>
          {%- render block -%}
        </div>
      {%- when 'custom_liquid' -%}
        <div class="custom-slot" {{ block.shopify_attributes }}>
          {{ block.settings.code }}
        </div>
    {%- endcase -%}
  {%- endfor -%}

</div>

