                        <p li-object="block.settings.text_paragraphe">{{ block.settings.text_paragraphe }}</p>
                      </article>
                    </div>
                  </div>
                </li>
                            
              
                    {% endfor %}    </ul>
            </div>
          </div>
        </div> {% schema %} {
    "tag": "section",
