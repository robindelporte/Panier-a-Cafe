                    <div x-bind:style="open === {{ forloop.index }} ? 'transform: rotate(180deg)' : 'transform: rotate(0deg)'" class="faq_icon w-embed"><svg width="24" height="25" viewbox="0 0 24 25" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <mask id="mask0_7620_8288" style="mask-type:alpha" maskunits="userSpaceOnUse" x="0" y="0" width="24" height="25">
                          <rect y="0.859375" width="24" height="24" fill="#D9D9D9"></rect>
                        </mask>
                        <g mask="url(#mask0_7620_8288)">
                          <path d="M11 13.7188H5V11.7188H11V5.71875H13V11.7188H19V13.7188H13V19.7188H11V13.7188Z" fill="currentColor"></path>
                        </g>
                      </svg></div>
                  </div>
                  <div id="faq-body-{{forloop.index}}" x-show="open === {{ forloop.index }}" x-collapse="" x-cloak="" aria-labelledby="faq-heading-{{forloop.index}}" class="faq_item-body">
                    <div class="tabs_panel-content is-team">
                      <div class="tabs_panel-image-wrapper"><img src="{{ block.settings.image_image | img_url: 'master' }}" loading="lazy" li-object:src="block.settings.image_image | img_url: 'master'" alt="" class="tabs_panel-image"></div>
                      <article class="tabs_panel-article">
